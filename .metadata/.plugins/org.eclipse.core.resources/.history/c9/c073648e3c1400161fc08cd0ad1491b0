package semExam;

public class Mainclass {

	public static void main(String[] args)
	{
		client mailbox1 = new client();
		client mailbox = new client();
		Server s1 = new Server("Panjim sub-Post office");
		Server s2 = new Server("Bicholim sub-Post office");
		
		s1.AddObserver(mailbox1);
		s2.AddObserver(mailbox);
		s1.newMail();
		s2.newMail();
		s1.RemoveObserver(mailbox1);
		//office1.RemoveObserver(mailbox1);
	}
}
