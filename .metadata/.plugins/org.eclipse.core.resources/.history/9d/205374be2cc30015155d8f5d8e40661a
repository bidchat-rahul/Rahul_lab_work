package tvRemoteStimulation;

public interface tvInterface {
	
	public void powerOn(tvControler tv);
	public void powerOff(tvControler tv);
	public void chennelUp(tvControler tv);
	public void chennelDown(tvControler tv);
	public void volumeUp(tvControler tv);
	public void volumeDown(tvControler tv);
	public void chennel(tvControler tv);
}
