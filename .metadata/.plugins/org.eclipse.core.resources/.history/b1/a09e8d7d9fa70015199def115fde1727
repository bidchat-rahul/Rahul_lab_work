package oOStack;

public class stackImplementation {
	private int n=5;
	private int Storage[] = new int[n] ;
	private int top = -1 ;
	
	public boolean pushS(int num){

		if(top == (n - 1)){
			return false;
		}
		else{
			Storage[++top]=num;
		}
			
		
		return true;
	}
	
	public boolean popS(){
		
		if(top == -1){
			return false;
			
		}
		else{
			top--;
		}
		
		return true;
	}
}
