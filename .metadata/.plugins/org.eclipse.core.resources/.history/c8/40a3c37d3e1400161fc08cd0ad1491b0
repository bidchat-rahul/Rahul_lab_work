package semExam;

public class Mainclass {

	public static void main(String[] args)
	{
		client counter1 = new client();
		client counter2 = new client();
		Server s1 = new Server("Panajim-counter");
		s1.setResult("400");
		
		s1.AddObserver(counter1);
		s1.AddObserver(counter2);
		
		s1.updateResult();
		s1.RemoveObserver(counter1);
		
		

	}
}
