package tvRemoteStimulation;

public class tvImplemantation {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Tv sony = new Tv();
		tvInterface remote  = new tvRemote();
		tvInterface tvControl  = new tvControler();
		
		remote.powerOn(sony);
		
		tvControl.powerOff(sony);
		

	}

}
