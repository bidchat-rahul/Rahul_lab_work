package testFactory;

public class factory {
	

	public Message getMessage(String MessageType) {
		if(MessageType == null)
		{
			return null;
		}
		
		if(MessageType.equalsIgnoreCase("Square")){
			return new Square();
		}
		
		if(MessageType.equalsIgnoreCase("Rectangle")){
			return new Rectangle();
		}
		
		return null;
	}


}
